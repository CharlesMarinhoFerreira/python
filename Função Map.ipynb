   "source": [
    "#Converter um valor de saída em mA para um instrumento com range (0 - 100 Kgf/cm²)\n",
    "\n",
    "pressao = [0,4,30,60,82,100]\n",
    "\n",
    "def converter(x):\n",
    "    return ((4*x)+100)/25\n",
    "    \n",
    "ma = list(map(converter,pressao))\n",
    "\n",
    "for a in ma:\n",
    "    print(a)"
